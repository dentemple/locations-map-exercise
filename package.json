{
  "name": "locations-map-exercise",
  "version": "1.0.0",
  "description": "A coding exercise using the BentoBox Locations API.",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "format": "prettier --single-quote --write  \"**/*.js\"",
    "format:check": "prettier --single-quote --list-different --write  \"**/*.js\"",
    "eslint": "eslint \"**/*.js\" --fix",
    "eslint:check": "eslint \"**/*.js\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "leaflet": "^1.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": ">=2",
    "lint-staged": ">=8",
    "prettier": "1.17.1",
    "react-hot-loader": "^4.8.8",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint \"**/*.js\" --fix",
      "prettier --single-quote  \"**/*.js\" --write",
      "git add"
    ]
  }
}
